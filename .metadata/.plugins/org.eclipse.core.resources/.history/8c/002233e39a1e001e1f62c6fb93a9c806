package FirstP;

public class plandaramAb {

	public static void main(String[] args) {
	
		
		int num = 22;
		int rev = 0;
		int real = num;
		
		
		
		while (num > 0) {
			
			rev = rev *10 + num % 10;
			//System.out.print(rev);
			num = num/ 10;
		} if (real == rev) {System.out.println(rev+"  plandram ");}else {System.out.println( "  Not Plandram");}
	}

}
